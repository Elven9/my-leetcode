{
  // Editor Setting
  "editor.tabCompletion": "off",
  "[cpp]": {
    "editor.quickSuggestions": {
      "other": "off",
      "comments": "off",
      "strings": "off"
    }
  },
  "[go]": {
    "editor.quickSuggestions": {
      "other": "off",
      "comments": "off",
      "strings": "off"
    }
  },
  "github.copilot.enable": {
    "*": false
  },
  "files.associations": {
    "*.cu": "cpp",
    "Makefile.inc": "makefile",
    "ostream": "cpp",
    "__hash_table": "cpp",
    "unordered_map": "cpp"
  }
}
